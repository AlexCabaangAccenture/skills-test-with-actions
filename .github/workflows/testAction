using CAT.App.D365.Service.Core.Utilities;
using Newtonsoft.Json;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CAT.App.D365.Service.Core.Models
{
    [JsonConverter(typeof(ServiceBusMessageResponseConverter))]
    public class ServiceBusMessageResponse
    {
        public string? BusinessEventId { get; set; }
        public string? BusinessEventLegalEntity { get; set; }
        public string? ContextRecordSubject { get; set; }
        public string? ControlNumber { get; set; }
        public string? curdoc { get; set; }
        public string? JournalName { get; set; }
        public string? linenumber { get; set; }
        public string? docdate { get; set; }
        public string? docnum { get; set; }
        public string? EventId { get; set; }
        public string? EventTime { get; set; }
        public string? EventTimeIso8601 { get; set; }
        public string? InitiatingUserAADObjectId { get; set; }
        public Key? Key { get; set; }
        public int? MajorVersion { get; set; }
        public int? MinorVersion { get; set; }
        public string? origdoccode { get; set; }
        public string? origdocnum { get; set; }
        public List<string>? ParentContextRecordSubjects { get; set; }
        public string? Reference1 { get; set; }
        public string? Reference2 { get; set; }
        public string? Reference3 { get; set; }
        public string? Reference4 { get; set; }
        public string? Reference5 { get; set; }
        public string? Reference6 { get; set; }
        public string? timestamp { get; set; }
        public string? username { get; set; }
        public double? value { get; set; }
        public double? valuedoc { get; set; }
        public string? xref { get; set; }
        public string? xrefname { get; set; }
    }

    public class Key
    {
        public string? CmpCode { get; set; }
        public string? Code { get; set; }
        public string? Number { get; set; }
    }
}
