using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CAT.App.D365.Service.Core.Models
{
    public class EMSReconBalanceRequest
    {
        public EMSReconBalance _request { get; set; }

        public EMSReconBalanceRequest(string company)
        {
            _request = new EMSReconBalance
            {
                Company = company
            };
        }
    }

    public class EMSReconBalance
    {
        public string Company { get; set; }
    }
}
